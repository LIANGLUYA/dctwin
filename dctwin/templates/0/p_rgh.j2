FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform {{ p_rgh }};

boundaryField
{
    {% for acu_boundary in acu_boundaries %}
    {{ acu_boundary.p_rgh }}
    {% endfor %}

    ".*"
    {
        type        fixedFluxPressure;
        value 		$internalField;
    }
}

