FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform {{ acu_k }};

boundaryField
{
    "acu_return_.*"
    {
        type            zeroGradient;
    }

    "acu_supply_.*"
    {
        type            fixedValue;
        value           uniform {{ acu_k }};
    }

    "acu_wall_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "room_wall_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "pillar_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "floor_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "ceiling_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "box_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "partition_wall_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "rack_wall_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "rack_panel_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "server_wall_.*"
    {
        type            kqRWallFunction;
        value           uniform {{ acu_k }};
    }

    "server_inlet_.*"
    {
        type            zeroGradient;
    }

    "server_outlet_.*"
    {
        type            fixedValue;
        value           uniform {{ server_k }};
    }
}


