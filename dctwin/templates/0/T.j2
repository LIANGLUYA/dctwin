FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   {{ 'uniform {}'.format(init_temperature) if not internal_field else internal_field }};

boundaryField
{

    {{ room_boundary.T }}

    {% for acu_boundary in acu_boundaries %}
    {{ acu_boundary.T }}
    {% endfor %}

    {% for server_boundary in server_boundaries %}
    {{ server_boundary.T }}
    {% endfor %}

}

